<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Facture.
    -->
    <changeSet id="20230523163815-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="facture"
                                 constraintName="fk_facture__client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>

        <addForeignKeyConstraint baseColumnNames="projet_id"
                                 baseTableName="facture"
                                 constraintName="fk_facture__projet_id"
                                 referencedColumnNames="id"
                                 referencedTableName="projet"/>
    </changeSet>
</databaseChangeLog>
